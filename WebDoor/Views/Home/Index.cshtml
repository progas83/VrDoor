    <section class="section section2 reserve" id="section2">
        <div class="container reserve__container">
            <h3>Забронировать время</h3>
            <div class="calendar__wrapper">
                <h3 id="month-name" class="calendar__month-name"></h3>
                <div id="calendar"></div>
                <button type="button" id="calendar-left" class="btn calendar__arrow calendar__arrow-left"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                <button type="button" id="calendar-right" class="btn calendar__arrow calendar__arrow-right"> <img src="data:image/svg+xml;utf8;base64," /> </button>
            </div>
            <!-- Modal start -->
            <div class="reserve__wrapper hide">
                <div>
                    <div>
                        <div>
                            <div class="row reserve__buttons-wrapper">
                                <div class="col-4">
                                    <div class="reserve__title">C какого времени</div>
                                    <div class="btn-group" role="group">
                                        <button type="button" class="btn btn-blue btn-minus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                        <span id="begin-time" class="time"></span>
                                        <button type="button" class="btn btn-blue btn-plus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                    </div>
                                </div>
                                <div class="col-4">
                                    <div class="reserve__title">До какого времени</div>
                                    <div class="btn-group" role="group">
                                        <button type="button" class="btn btn-blue btn-minus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                        <span id="end-time" class="time"></span>
                                        <button type="button" class="btn btn-blue btn-plus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                    </div>
                                </div>
                                <div class="col-4">
                                    <div class="reserve__title">Количество человек</div>
                                    <div class="btn-group" role="group">
                                        <button type="button" class="btn btn-blue btn-minus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                        <span id="men-quantity" class="men-quantity"></span>
                                        <button type="button" class="btn btn-blue btn-plus"> <img src="data:image/svg+xml;utf8;base64," /> </button>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div id="time-list" class="row reserve__time-list"></div>
                        <div class="row">
                            <div class="col-6"><input id="name" type="text" name="name" placeholder="Имя"></div>
                            <div class="col-6"><input id="tel" type="tel" name="tel" placeholder="Телефон"></div>
                        </div>
                        <div class="reserve__buttons">
                            <div class="reserve__message"></div>
                            <button id="back-button" type="button" class="btn btn-blue">Назад</button>
                            <button id="reserve-button" type="button" class="btn btn-cta">Бронировать</button>
                        </div>
                    </div>
                </div>
            </div>
            <!-- Modal end -->
        </div>
    </section>
    <section class="row contacts section section3" id="section3">
        <div class="container">
            <h3>Мы открыты</h3>
            <p class="time">
                Пн.-Пт. 14:00 - 23:00 Сб.-Вс. 10:00- 00:00
            </p>
